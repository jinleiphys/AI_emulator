NAMELIST
! Random J sampling emulator training test

&EMULATOR_TRAIN
  base_input = '../test/test_d58Ni_cdcc.in'
  n_samples = 10
  n_basis = 10
  method = 'svd'
  tol = 1.0d-8
  output_file = 'emulator_randomJ_test.dat'
  multi_J = .true.
  random_J = .true.
  n_J_samples = 5
/

! Parameter range covering +/- 10% variation
&PARAM_RANGE  name='t_1_uv', min_val=48.0, max_val=58.0 /
&PARAM_RANGE  name='t_1_wd', min_val=6.0, max_val=10.0 /
&PARAM_RANGE /
